    </statBases>
    <uiIconScale>2</uiIconScale>
    <tools>
      <li Class="CombatExtended.ToolCE">
        <label>mandibles</label>
        <capacities>
          <li>ToxicBite</li>	<!-- Токсичный укус, взял у токсичной крысы -->
