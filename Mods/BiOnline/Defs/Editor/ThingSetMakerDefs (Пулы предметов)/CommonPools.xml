              <filter>
                <categories>      
                  <li>Foods</li>
                </categories>
                <disallowedThingDefs>
                  <li>InsectJelly</li>
				  <li>MealNutrientPaste</li>
                </disallowedThingDefs>
                <disallowWorsePreferability>RawBad</disallowWorsePreferability>
