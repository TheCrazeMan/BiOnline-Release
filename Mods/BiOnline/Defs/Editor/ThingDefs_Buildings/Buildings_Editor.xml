	<ThingDef ParentName="BuildingBase">
		<defName>PowerPoint</defName>
		<label>Power point</label>
		<description>Map object with ulimited power generation</description>
		<thingClass>Building</thingClass>
		<category>Building</category>
		<graphicData>
		  <texPath>Editor/PGE_SettlementPawnSpawnPoint</texPath>
		  <graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<uiIconPath>Things/Building/Linked/WallSmooth_MenuIcon</uiIconPath>
		<useHitPoints>false</useHitPoints>
		<leaveResourcesWhenKilled>false</leaveResourcesWhenKilled>
		<altitudeLayer>Conduits</altitudeLayer>
		<minifiedDef />	
		<passability>Standable</passability>
		<tickerType>Normal</tickerType>
		<rotatable>false</rotatable>
		<selectable>true</selectable>
		<neverMultiSelect>false</neverMultiSelect>
		<holdsRoof>false</holdsRoof>
		<staticSunShadowHeight>1.0</staticSunShadowHeight>
		<blockLight>false</blockLight>
		<canOverlapZones>True</canOverlapZones>
		<drawGUIOverlay>true</drawGUIOverlay>
		<comps>
		  <li Class="CompProperties_Lifespan">
			<lifespanTicks>300000</lifespanTicks>
		  </li>
		  <li Class="CompProperties_Power">
			<compClass>CompPowerPlant</compClass>
			<basePowerConsumption>-99999</basePowerConsumption>
			<transmitsPower>true</transmitsPower>
		  </li>		  
		</comps>
		<building>
		  <isEdifice>false</isEdifice>
		</building>
		
<!-- 		<designationCategory>Misc</designationCategory>		 -->
	
	</ThingDef>
	
</Defs>