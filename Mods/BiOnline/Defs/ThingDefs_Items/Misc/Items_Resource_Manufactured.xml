<Defs>
	<!--Хитин-->
	<ThingDef ParentName="ResourceBase">
		<defName>Chitin</defName>
		<label>Chitin</label>
		<description>Chitin is a strong and flexible material that is extracted from beetle shells. It is highly resistant to damage and environmental influences. This material is often used in the creation of lightweight and durable armor.</description>
		<graphicData>
			<texPath>Things/Item/Chitin</texPath>
			<graphicClass>Graphic_StackCount</graphicClass>
		</graphicData>
		<soundInteract>Standard_Drop</soundInteract>
		<soundDrop>Standard_Drop</soundDrop>
		<stackLimit>75</stackLimit>
		<healthAffectsPrice>false</healthAffectsPrice>
		<tradeability>Sellable</tradeability>
		<statBases>
			<MaxHitPoints>70</MaxHitPoints>
			<MarketValue>2</MarketValue>
			<Mass>0.5</Mass>
			<Flammability>0.2</Flammability>
			<DeteriorationRate>0</DeteriorationRate>
		</statBases>
		<stuffProps>
		  <categories>
			<li>SoftArmor</li>
			<li>Fabric</li>
		  </categories>
		  <commonality>0.15</commonality>
		  <color>(113,90,61)</color>
		  <statFactors>
			<Flammability>0.2</Flammability>
			<MaxHitPoints>1.55</MaxHitPoints>
			<Mass>1.25</Mass>
			<Bulk>1.2</Bulk>
			<WornBulk>1.2</WornBulk>
		  </statFactors>
		</stuffProps>
		<intricate>false</intricate>
		<thingCategories>
			<li>Manufactured</li>
		</thingCategories>
	</ThingDef>

