<!-- Shrooms -->
<ThingDef ParentName="RoughPlantBase">
	<defName>TM_RawShrooms</defName>
	<label>Shrooms</label>
	<description>Small mushrooms used by tribespeople to make medical beverages, useless for most advanced civilizations.</description>
	<stackLimit>75</stackLimit>
	<graphicData>
		<texPath>Things/Plant/Raw/Shrooms_Raw</texPath>
		<graphicClass>Graphic_Single</graphicClass>
	</graphicData>
	<statBases>
		<MarketValue>0.2</MarketValue>
		<Mass>0.008</Mass>
		<Flammability>1.8</Flammability>
	</statBases>
	<comps>
		<li Class="CompProperties_Rottable">
			<daysToRotStart>70</daysToRotStart>
			<rotDestroys>true</rotDestroys>
		</li>
	</comps>
	<thingCategories>
		<li>TM_MedicalHerbs</li>
	</thingCategories>
	<ingestible>
		<optimalityOffsetFeedingAnimals>-12</optimalityOffsetFeedingAnimals>
		<preferability>NeverForNutrition</preferability>
		<outcomeDoers>
        <li Class="IngestionOutcomeDoer_GiveHediff">
          <hediffDef>Plant_ShroomsPoison</hediffDef>
          <severity>0.1</severity>
        </li>
      </outcomeDoers>
	</ingestible>
</ThingDef>

