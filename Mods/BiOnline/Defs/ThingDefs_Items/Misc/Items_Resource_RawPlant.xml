<?xml version="1.0" encoding="utf-8" ?>
<Defs>

<!-- Broots -->
<ThingDef ParentName="RoughPlantBase">
	<defName>TM_RawBroots</defName>
	<label>White Broots</label>
	<description>Fibrous white broots that grow wildly.\nTribal people hold the knowledge to use them to prepare medical beverages.</description>
	<stackLimit>75</stackLimit>
	<graphicData>
		<texPath>Things/Plant/Raw/Broots_Raw</texPath>
		<graphicClass>Graphic_Single</graphicClass>
	</graphicData>
	<statBases>
		<MarketValue>0.4</MarketValue>
		<Mass>0.010</Mass>
		<Flammability>1.2</Flammability>
	</statBases>
	<comps>
		<li Class="CompProperties_Rottable">
			<daysToRotStart>80</daysToRotStart>
			<rotDestroys>true</rotDestroys>
		</li>
	</comps>
	<thingCategories>
		<li>TM_MedicalHerbs</li>
	</thingCategories>
	<ingestible>
		<optimalityOffsetFeedingAnimals>-12</optimalityOffsetFeedingAnimals>
		<preferability>NeverForNutrition</preferability>
	</ingestible>
</ThingDef>

