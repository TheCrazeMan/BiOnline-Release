<Defs>

	<ThingDef Abstract="True" ParentName="BedWithQualityBase" Name="Bed_Neolit"> <!-- Needs to re-reference parent -->
		<designationCategory>Furniture</designationCategory>
		<statBases>
			<StyleDominance MayRequire="Ludeon.RimWorld.Ideology">5</StyleDominance>
			<BirthRitualQualityOffset MayRequire="Ludeon.RimWorld.Biotech">2</BirthRitualQualityOffset>
		</statBases>
	</ThingDef>


	<!--Соломенная кровать-->
	<ThingDef ParentName="Bed_Neolit">
		<defName>StrawBed</defName>
		<label>Straw bed</label>
		<description>A simple and quick to make wicker bed, padded with straw for added comfort.</description>
		<graphicData>
			<texPath>Things/Build/Furniture/StrawBed/StrawBed</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<shaderType>CutoutComplex</shaderType>
			<drawSize>(2,2)</drawSize>
			<damageData>
				<rect>(0,0.05,1,1.95)</rect>
			</damageData>
		</graphicData>

		<comps>
			<li Class="CompProperties_AffectedByFacilities">
				<linkableFacilities>
					<li>Dresser</li>
					<li>EndTable</li>
				</linkableFacilities>
			</li>
		</comps>

		<statBases>
			<MaxHitPoints>120</MaxHitPoints>
			<Flammability>1.0</Flammability>
			<Beauty>1</Beauty>
			<WorkToBuild>650</WorkToBuild>
			<Mass>12</Mass>
			<BedRestEffectiveness>0.80</BedRestEffectiveness>
			<Comfort>0.50</Comfort>
			<ImmunityGainSpeedFactor>0.85</ImmunityGainSpeedFactor>
			<SurgerySuccessChanceFactor>0.2</SurgerySuccessChanceFactor>
		</statBases>

		<size>(1,2)</size>
		<stuffCategories>
			<li>Metallic</li>
			<li>Woody</li>
			<li>Stony</li>
		</stuffCategories>
		<costStuffCount>45</costStuffCount>
		<costList>
			<Hay>20</Hay>
		</costList>
		<building>
			<bed_showSleeperBody>True</bed_showSleeperBody>
			<bed_healPerDay>4</bed_healPerDay>
		</building>
		<fillPercent>0.4</fillPercent>
		<pathCost>42</pathCost>
		<staticSunShadowHeight>0.17</staticSunShadowHeight>
		<castEdgeShadows>True</castEdgeShadows>
		<designationHotKey>Misc2</designationHotKey>
		<uiIconScale>0.90</uiIconScale>
		<designatorDropdown>Furniture_SingleBed</designatorDropdown>
	</ThingDef>
	
	
