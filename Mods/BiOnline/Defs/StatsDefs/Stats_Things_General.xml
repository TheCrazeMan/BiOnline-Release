	<StatDef>
		<defName>OreGridCommonality</defName>
		<label>Ore grid commonality</label>
		<description>Reflects how often the ore is found in deposits underground.</description>
		<category>BasicsNonPawn</category>
		<defaultBaseValue>0</defaultBaseValue>
		<minValue>0</minValue>
		<roundValue>true</roundValue>
		<toStringStyle>FloatMaxTwo</toStringStyle>
		<showNonAbstract>true</showNonAbstract>
		<scenarioRandomizable>true</scenarioRandomizable>
