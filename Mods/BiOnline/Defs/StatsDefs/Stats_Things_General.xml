<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<StatDef>
		<defName>Nutrients</defName>
		<label>Nutrients</label>
		<description>How much does the food balance when consumed</description>
		<category>BasicsNonPawn</category>
		<defaultBaseValue>0</defaultBaseValue>
		<hideAtValue>0</hideAtValue>
		<minValue>0</minValue>
		<toStringStyle>FloatTwo</toStringStyle>
		<showOnPawns>true</showOnPawns>
		<displayPriorityInCategory>3000</displayPriorityInCategory>
		<forInformationOnly>true</forInformationOnly>
	</StatDef>
	
	<StatDef>
		<defName>NutrientsLimit</defName>
