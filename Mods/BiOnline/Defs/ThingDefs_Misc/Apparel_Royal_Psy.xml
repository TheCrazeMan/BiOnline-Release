<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<ThingDef ParentName="ApparelBase">
		<defName>Apparel_Crown_PSY</defName>
		<label>crown</label>
		<description>A crown of metal with an elaborate design.</description>
		<descriptionHyperlinks><AbilityDef>WordOfSerenity_PSY</AbilityDef></descriptionHyperlinks>
		<smeltable>false</smeltable>
		<thingCategories>
		  <li>HeadgearNoble</li>
		</thingCategories>
		<graphicData>
		  <texPath>Things/Pawn/Humanlike/Apparel/Crown/Crown</texPath>
		  <graphicClass>Graphic_Single</graphicClass>
		  <drawSize>(0.9, 0.9)</drawSize>
		</graphicData>
		<statBases>
		  <MaxHitPoints>80</MaxHitPoints>
		  <WorkToMake>12000</WorkToMake>
		  <Mass>0.9</Mass>
		  <EquipDelay>0.8</EquipDelay>
		</statBases>
		<tradeability>Sellable</tradeability>
		<equippedStatOffsets>
		  <SocialImpact>0.2</SocialImpact>
		  <SlaveSuppressionOffset MayRequire="Ludeon.RimWorld.Ideology">-0.3</SlaveSuppressionOffset>
		</equippedStatOffsets>
		<apparel>
		  <countsAsClothingForNudity>false</countsAsClothingForNudity>
		  <bodyPartGroups>
			<li>UpperHead</li>
		  </bodyPartGroups>
		  <wornGraphicPath>Things/Pawn/Humanlike/Apparel/Crown/Crown</wornGraphicPath>
		  <layers>
			<li>Overhead</li>
		  </layers>
		  <tags>
			<li>Royal</li>
			<li>RoyalTier6</li>
		  </tags>
		  <canBeDesiredForIdeo>false</canBeDesiredForIdeo>
		  <canBeGeneratedToSatisfyWarmth>false</canBeGeneratedToSatisfyWarmth>
		  <canBeGeneratedToSatisfyToxicEnvironmentResistance>false</canBeGeneratedToSatisfyToxicEnvironmentResistance>
		</apparel>
		<colorGenerator Class="ColorGenerator_Options">
			<options>
				<li>
					<weight>10</weight>
					<only>(231,196,70)</only>
				</li>
			</options>
		</colorGenerator>
		<tradeTags>
		  <li>Clothing</li>
		</tradeTags>
		<comps>
			<li Class="GiveThingsPsycasts.CompProperties_GiveThingsPsycasts">
			<compClass>GiveThingsPsycasts.CompGiveThingsPsycasts</compClass>
			<PsycastDefName>WordOfSerenity_PSY</PsycastDefName>
			</li>
		</comps>
	</ThingDef>

