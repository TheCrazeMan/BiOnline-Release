			  <hediffDef>Cyberpsychosis</hediffDef>
			  <severity>-0.25</severity>
			</li>
		  </outcomeDoers>
		</ingestible>
		<tradeability>Sellable</tradeability>
		<costList>
			<TM_RawShrooms>5</TM_RawShrooms>
			<Neutroamine>10</Neutroamine>
		</costList>
		<recipeMaker>
			<researchPrerequisite>RE_ImmunoBlockers</researchPrerequisite>
			<bulkRecipeCount>4</bulkRecipeCount>
		  <recipeUsers>
			<li>MedicalBench</li>
		  </recipeUsers>
		  <soundWorking>Recipe_Drug</soundWorking>
		</recipeMaker>
		<comps>
		  <li Class="CompProperties_Drug">
			<listOrder>1000</listOrder>
			<overdoseSeverityOffset>0</overdoseSeverityOffset>
		  </li>
		  <li Class="CompProperties_Rottable">
			<daysToRotStart>6</daysToRotStart>
			<rotDestroys>false</rotDestroys>
		  </li>
		</comps>
	</ThingDef>
	
