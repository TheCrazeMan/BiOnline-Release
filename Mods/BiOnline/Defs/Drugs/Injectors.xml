  <HediffDef>
    <defName>DefleXitol_Effect</defName>
    <hediffClass>Hediff_High</hediffClass>
    <label>DefleXitol effect</label>
    <description>This pawn is under the effect of the DefleXitol injector. It will have lesser damage from all sources.</description>
    <defaultLabelColor>(255,188,188)</defaultLabelColor>
    <scenarioCanAdd>true</scenarioCanAdd>
    <maxSeverity>1.0</maxSeverity>
    <isBad>false</isBad>
