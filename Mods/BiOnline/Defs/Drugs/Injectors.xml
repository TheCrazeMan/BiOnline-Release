<Defs>

	<!-- База для инъекторов -->

	<ThingDef Name="BaseDrugInjector" Abstract="True">
	<thingClass>ThingWithComps</thingClass>
	<category>Item</category>
	<drawerType>MapMeshOnly</drawerType>
		<graphicData>
		  <texPath>Things/Item/Drug/Injector</texPath>
		  <graphicClass>Graphic_StackCount</graphicClass>
		  <shaderType>CutoutComplex</shaderType>
		   <drawSize>0.8</drawSize>
		</graphicData>	
	<techLevel>Ultra</techLevel>
	<tradeability>Sellable</tradeability>
    <altitudeLayer>Item</altitudeLayer>
    <selectable>true</selectable>
	<useHitPoints>true</useHitPoints>
    <stackLimit>75</stackLimit>
		<statBases>
			<SellPriceFactor>0.25</SellPriceFactor>
		    <MaxHitPoints>50</MaxHitPoints>
			<Flammability>1.0</Flammability>
			<DeteriorationRate>2</DeteriorationRate>
			<Beauty>-4</Beauty>
			<WorkToMake>800</WorkToMake>
			<MarketValue>175</MarketValue>
			<Mass>0.01</Mass>
			<Bulk>0.1</Bulk>
		</statBases>
		<ingestible>
		  <preferability>NeverForNutrition</preferability>
		  <drugCategory>Medical</drugCategory>
		  <foodType>Processed, Fluid</foodType>
		  <baseIngestTicks>20</baseIngestTicks>
		  <ingestSound>Ingest_Inject</ingestSound>
		  <ingestHoldOffsetStanding>
			<northDefault>
			  <offset>(0.18,0,0)</offset>
			</northDefault>
		  </ingestHoldOffsetStanding>
		  <maxNumToIngestAtOnce>1</maxNumToIngestAtOnce>
		  <ingestCommandString>Inject {0}</ingestCommandString>
		  <ingestReportString>Injecting {0}.</ingestReportString>
		  <useEatingSpeedStat>false</useEatingSpeedStat>
		  <chairSearchRadius>0</chairSearchRadius>
		</ingestible>
		 <comps>
		  <li Class="CompProperties_Drug">
		   <listOrder>100</listOrder>
		  </li>
		  <li Class="CompProperties_Forbiddable" />
		</comps>
    <thingCategories>
      <li>Drugs</li>
    </thingCategories>
    <alwaysHaulable>true</alwaysHaulable>	
    <pathCost>14</pathCost>
    <burnableByRecipe>true</burnableByRecipe>
    <resourceReadoutPriority>Last</resourceReadoutPriority>
    <drawGUIOverlay>true</drawGUIOverlay>
    <storedConceptLearnOpportunity>DrugPolicies</storedConceptLearnOpportunity>
    <orderedTakeGroup>Drug</orderedTakeGroup>	
	</ThingDef>

