						<offset>(0.05,0)</offset>
					</thin>
					<hulk>
						<offset>(-0.1,0)</offset>
					</hulk>
					<fat>
						<offset>(-0.15,0)</offset>
					</fat>
				</east>
				<west>
					<offset>(0.35,-0.05)</offset>
					<thin>
						<offset>(-0.05,0)</offset>
					</thin>
					<hulk>
						<offset>(0.1,0)</offset>
					</hulk>
					<fat>
						<offset>(0.15,0)</offset>
					</fat>
				</west>
				<male>
					<scale>(0.7,0.7)</scale>
				</male>
				<female>
					<scale>(0.7,0.7)</scale>
				</female>
				<thin>
					<scale>(0.65,0.65)</scale>
				</thin>
				<hulk>
					<scale>(0.85,0.85)</scale>
