			<EquipDelay>8</EquipDelay>
		</statBases>
		<equippedStatOffsets>
			<IncomingDamageFactor>-0.25</IncomingDamageFactor>
			<PainShockThreshold>0.25</PainShockThreshold>
		</equippedStatOffsets>
		<thingCategories>
			<li>ApparelUtility</li>
		</thingCategories>
		<apparel>
			<wornGraphicPath>Things/Pawn/Humanlike/Apparel/Body/Backpack/Spacer/TraumaKit/TraumaKit2</wornGraphicPath>
			<useWornGraphicMask>true</useWornGraphicMask>
