		<tradeability>Sellable</tradeability>
		<colorGenerator Class="ColorGenerator_Options">
			<options>
				<li>
					<weight>2</weight>
					<only>(102,144,150)</only>
				</li>
				<li>
					<weight>3</weight>
					<only>(159,159,159)</only>
				</li>
				<li>
					<weight>0.1</weight>
					<only>(255,192,203)</only>
				</li>
			</options>
		</colorGenerator>
	</ThingDef>
	
	
	
