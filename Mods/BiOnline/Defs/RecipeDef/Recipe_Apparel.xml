<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	
	<RecipeDef>
		<defName>Make_Apparel_Crown_PSY</defName>
		<label>Make psionic crown</label>
		<description>Make psionic crown.</description>
		<jobString>Make psionic crown.</jobString>
		<effectWorking>Smelt</effectWorking>
		<soundWorking>Recipe_Smelt</soundWorking>
		<workAmount>30000</workAmount>
		<workSpeedStat>GeneralLaborSpeed</workSpeedStat>
		<researchPrerequisite>Awakening_PSY</researchPrerequisite>
		<recipeUsers>
			<li>ElectricSmithy</li>
			<li>FueledSmithy</li>
		</recipeUsers>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Psi_Fiber</li>
					</thingDefs>
				</filter>
				<count>25</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Chlorophyte</li>
					</thingDefs>
				</filter>
				<count>50</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Gold</li>
					</thingDefs>
				</filter>
				<count>100</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
		  <thingDefs>
			<li>Gold</li>
			<li>Chlorophyte</li>
			<li>Psi_Fiber</li>
		  </thingDefs>
		</fixedIngredientFilter>
		<products>
			<Apparel_Crown_PSY>1</Apparel_Crown_PSY>
		</products>
	</RecipeDef>
	
