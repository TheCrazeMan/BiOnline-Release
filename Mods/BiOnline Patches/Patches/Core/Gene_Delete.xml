<Patch>

	<Operation Class="PatchOperationAdd">
	<xpath>Defs/GeneDef[defName="ArchiteMetabolism" or defName="Deathless" or defName="Ageless" or defName="TotalHealing" or defName="DiseaseFree" or defName="PerfectImmunity" or defName="XenogermReimplanter" or defName="RobustDigestion" or defName="PsychicBonding" or defName="Hemogenic" or defName="HemogenDrain" or defName="Deathrest" or defName="Coagulate" or defName="Bloodfeeder" or defName="LongjumpLegs" or defName="PiercingSpine" or defName="Superclotting" or defName="WoundHealing_SuperFast" or defName="Immunity_SuperStrong" or defName="Mood_Sanguine" or defName="KillThirst" or defName="ViolenceDisabled" or defName="MeleeDamage_Strong" or defName="MeleeDamage_Weak" or defName="Neversleep" or defName="Robust" or defName="Learning_Fast"]</xpath>
		<value> 

			<modExtensions>
			<li Class="BiOnline.Genes.DefModExtension_GeneDef">
			<showInEditor>false</showInEditor>
			</li>
			</modExtensions>

		</value>
	</Operation>
  
	<Operation Class="PatchOperationAdd">
		<xpath>Defs/GeneTemplateDef[defName="AptitudeTerrible" or defName="AptitudePoor" or defName="AptitudeStrong" or defName="AptitudeRemarkable"]</xpath>
