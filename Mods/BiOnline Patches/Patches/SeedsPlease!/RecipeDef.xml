<?xml version="1.0" encoding="utf-8"?>
<Patch>
	
	<Operation Class="PatchOperationReplace">
		<xpath>Defs/RecipeDef[defName="ExtractSeeds"]</xpath>
		<value>

			<RecipeDef>
				<defName>ExtractSeeds</defName>
				<label>extract seeds</label>
				<description>Extract seeds from produce.</description>
				<jobString>Extracting seeds.</jobString>
				<workSpeedStat>CookSpeed</workSpeedStat>
				<effectWorking>Cook</effectWorking>
				<workSkill>Cooking</workSkill>
				<workAmount>500</workAmount>
				<recipeUsers>
					<li>FueledStove</li>
					<li>ElectricStove</li>
					<li>Campfire</li>
					<li>FirePit</li>
					<li>CraftingSpot</li>
				</recipeUsers>
				<ingredients>
					<li>
						<filter>
							<categories>
								<li>SeedExtractable</li>
							</categories>
						</filter>
						<count>8</count>
					</li>
				</ingredients>
				<fixedIngredientFilter>
					<categories>
						<li>SeedExtractable</li>
					</categories>
				</fixedIngredientFilter>
				<defaultIngredientFilter>
					<disallowedCategories>
						<li>SeedExtractable</li>
					</disallowedCategories>
				</defaultIngredientFilter>
				<specialProducts>
					<li>Butchery</li>
				</specialProducts>
				<workerCounterClass>SeedsPleaseLite.RecipeWorkerCounter_ExtractSeeds</workerCounterClass>
				<modExtensions>
					<li Class="BiOnline.Recipes.DefModExtension_RecipeDef">
						<buildingTabCategoryDef>Seeds</buildingTabCategoryDef>
					</li>
				</modExtensions>	
			</RecipeDef>
		
		</value>
	</Operation>
	
	<Operation Class="PatchOperationRemove">
		<xpath>Defs/RecipeDef[defName="RefineBerries" or defName="RefineFungus"]</xpath>
	</Operation>
	
</Patch>